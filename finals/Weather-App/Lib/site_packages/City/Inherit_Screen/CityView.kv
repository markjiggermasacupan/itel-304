#:import webbrowser webbrowser
#: include Lib\site_packages\City\Inherit_Screen\bar_view.kv
<CityView>:
	

	MDBoxLayout:
		orientation : 'vertical'
		#md_bg_color : 1,0,1,1

		MDToolbar:
			id: go_back_tool_bar
			title: 'Your Cities'
			elevation: 10
			pos_hint: {"top":1}
			left_action_items: [['keyboard-backspace', lambda x: root.go_back()]]

		

		MDLabel:
			text: ''

	MDBoxLayout:
		orientation: 'vertical'
		#md_bg_color : 1,0,1,1
		pos_hint: {'top':0.88, 'left':1}

		MDLabel:
			id : city_text
			text : 'welcome'
			halign: 'center'
			size_hint_y: None
			height : 30
		MDSeparator:
			height: "1dp"
			size_hint_x: None
			width: 300
			pos_hint : {'center_x': 0.5, 'center_y': 0.5}

		CityView_content :
			id : city_content

		

		MDLabel:
			text: ''
			size_hint_y:None
			height: 100





<CityView_content>:
	

	ScrollView:
